/*
 * DEFAULT TERRA++ TREE COVER DATASETS
 */

[
    //whole world at max resolution
    {
        "dataset": {
            "urls": [
                "https://cloud.daporkchop.net/gis/treecover2000/${x}/${z}.tiff"
            ],
            "projection": {
                "scale": {
                    "delegate": {
                        "equirectangular": {}
                    },
                    "x": 6250.0,
                    "y": 6250.0
                }
            },
            "resolution": 256,
            "blend": "CUBIC",
            "parse": {
                "divide": {
                    "delegate": {
                        "from_int": {
                            "delegate": {
                                "flip_z": {
                                    "delegate": {
                                        "grayscale_extract": {
                                            "delegate": {
                                                "parse_tiff": {},
                                            }
                                        }
                                    }
                                }
                            }
                        }
                    },
                    "value": 100.0
                }
            }
        },
        "bounds": {
            "minX": -180.0,
            "maxX": 180.0,
            "minZ": -60,
            "maxZ": 80
        },
        "priority": -100.0
    }
]
